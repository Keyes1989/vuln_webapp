{
    "name": "andrew-miller/example-php-project",
    "description": "A basic example of a modern PHP web app.",
    "authors": [
        {
            "name": "Miller, Andrew",
            "email": "andrew.miller@rakuten.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    }
}
